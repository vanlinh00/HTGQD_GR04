        <div class="card-body pb-0">
          <h5 class="card-title">Bảng Quyết định Phương pháp TOP-SIS</h5>
          <table class="table table-borderless">
            <thead>
              <tr>
                <th scope="col">STT</th>
                <th scope="col">Vùng</th>
                <th scope="col">Số Bệnh Nhân</th>
                <th scope="col">Số Bác Sĩ</th>
                <th scope="col">TL Bệnh nhân/ Bác sĩ</th>
                <th scope="col">TT Bệnh Nhân</th>
                <th scope="col">S*</th>
                <th scope="col">S-</th>
                <th scope="col">C*</th>
              </tr>
            </thead>
            <tbody>
              <% if(getAllRegionAndPatient.length!=0){ var i=1; getAllRegionAndPatient.forEach(function(data){ %>
                <tr>
                  <td>
                    <%=i%>
                  </td>
                  <td>
                    <%=data.name_region %>
                  </td>
                  <td>
                    <%=data.patient%>
                  </td>
                  <td>
                    <%=data.doctor%>
                  </td>
                  <td>
                    <%=data.ratioOfPatientsToDoctors%>
                  </td>
                  <td>
                    <%=data.p_condition%>
                  </td>
                  <td>
                    <%=data.sStart_of_region%>
                  </td>
                  <td>
                    <%=data.sMinus_of_region%>
                  </td>
                  <td>
                    <%=data.cStars_of_region%>
                  </td>
                </tr>
                <% i++; }) %>
                  <% } else{ %>
                    <tr>

                    </tr>
                    <% } %>

            </tbody>
          </table>

        </div>

